{
  "name": "example-cicd-app",
  "version": "1.0.0",
  "description": "Example application for CI/CD pipeline",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest",
    "build": "rm -rf dist && mkdir -p dist && cp -r app.js package*.json dist/ && cd dist && npm install --production && cd .. && zip -r dist/app.zip dist/"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  }
}
